<%@ page language="java" contentType="text/html; charset=ISO-8859-1"
    pageEncoding="ISO-8859-1"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="ISO-8859-1">
<title>SignUp Page</title>
</head>
<body>
<h1>This is SignUp Page</h1>
<form action="signup">

<table>
<tr>
<td>Employee Name:</td>
<td><input type="text" name="ename" placeholder="Enter employee name"></td>
</tr>
<tr>
<td>Employee Address:</td>
<td><input type="text" name="address" placeholder="Enter employee address"></td>
</tr>
<tr>
<td>Employee Username:</td>
<td><input type="text" name="eusername" placeholder="Enter employee username"></td>
</tr>
<tr>
<td>Employee Password:</td>
<td><input type="text" name="epassword" placeholder="Enter employee password"></td>
</tr>
<tr>
<td>Employee Age:</td>
<td><input type="text" name="eage" placeholder="Enter employee age"></td>
</tr>

<tr align="center">
<td></td>
<td><input type="submit" value="SingUp"></td>
</tr>

</table>

</form>


</body>
</html>